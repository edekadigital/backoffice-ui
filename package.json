{
  "name": "@edekadigital/backoffice-ui",
  "description": "React components based on Material Design",
  "version": "0.20.3",
  "license": "MIT",
  "author": "EDEKA DIGITAL GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/edekadigital/backoffice-ui.git"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/types/index.d.ts",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "storybook": "start-storybook -p 8000 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o public",
    "build": "npm run build-bundles && npm run build-types",
    "build-bundles": "rollup -c",
    "build-types": "tsc -p tsconfig.types.json --outDir ./lib/types",
    "test": "jest",
    "check": "gts check",
    "fix": "gts fix",
    "clean": "gts clean",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "gts fix",
      "git add"
    ],
    "*.{json,md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.12.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "gts": "^1.0.0-0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.3",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@material-ui/core": "^4.1.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.1.2",
    "notistack": "^0.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}

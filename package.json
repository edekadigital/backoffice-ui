{
  "name": "@edekadigital/backoffice-ui",
  "description": "React components based on Material Design",
  "version": "0.22.0",
  "license": "MIT",
  "author": "EDEKA DIGITAL GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/edekadigital/backoffice-ui.git"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/types/index.d.ts",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 8000 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o public",
    "build": "npm run build-bundles && npm run build-types",
    "build-bundles": "rollup -c",
    "build-types": "tsc -p tsconfig.types.json --outDir ./lib/types",
    "test": "jest",
    "check": "gts check",
    "fix": "gts fix",
    "clean": "gts clean",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "gts fix",
      "git add"
    ],
    "*.{json,md,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/react": "^5.3.8",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "gts": "^1.1.2",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/lab": "^4.0.0-alpha.40",
    "@material-ui/styles": "^4.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
